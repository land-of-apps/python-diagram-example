    {
      "sql_query": {
        "sql": "SELECT \"offer_range\".\"id\", \"offer_range\".\"name\", \"offer_range\".\"slug\", \"offer_range\".\"description\", \"offer_range\".\"is_public\", \"offer_range\".\"includes_all_products\", \"offer_range\".\"proxy_class\", \"offer_range\".\"date_created\" FROM \"offer_range\" LEFT OUTER JOIN \"offer_rangeproduct\" ON (\"offer_range\".\"id\" = \"offer_rangeproduct\".\"range_id\") LEFT OUTER JOIN \"offer_range_included_categories\" ON (\"offer_range\".\"id\" = \"offer_range_included_categories\".\"range_id\") LEFT OUTER JOIN \"offer_range_classes\" ON (\"offer_range\".\"id\" = \"offer_range_classes\".\"range_id\") WHERE ((NOT (EXISTS(SELECT 1 AS \"a\" FROM \"offer_range_excluded_products\" U1 WHERE (U1.\"product_id\" = 904 AND U1.\"range_id\" = (\"offer_range\".\"id\")) LIMIT 1)) AND \"offer_range\".\"includes_all_products\") OR (NOT (EXISTS(SELECT 1 AS \"a\" FROM \"offer_range_excluded_products\" U1 WHERE (U1.\"product_id\" = 904 AND U1.\"range_id\" = (\"offer_range\".\"id\")) LIMIT 1)) AND (\"offer_rangeproduct\".\"product_id\" = 904 OR \"offer_range_included_categories\".\"category_id\" IN (\n(SELECT \"CATALOGUE_CATEGORY_JOIN\".\"id\" FROM \"catalogue_category\" AS \"CATALOGUE_CATEGORY_BASE\"\nLEFT JOIN \"catalogue_category\" AS \"CATALOGUE_CATEGORY_JOIN\" ON (\n    \"CATALOGUE_CATEGORY_BASE\".\"path\" LIKE \"CATALOGUE_CATEGORY_JOIN\".\"path\" || '%'\n    AND \"CATALOGUE_CATEGORY_BASE\".\"depth\" >= \"CATALOGUE_CATEGORY_JOIN\".\"depth\"\n)\nWHERE \"CATALOGUE_CATEGORY_BASE\".\"id\" IN (SELECT U0.\"id\" FROM \"catalogue_category\" U0 INNER JOIN \"catalogue_productcategory\" U1 ON (U0.\"id\" = U1.\"category_id\") WHERE U1.\"product_id\" = 904 ORDER BY U0.\"path\" ASC))\n) OR \"offer_range_classes\".\"productclass_id\" = 684) AND NOT \"offer_range\".\"includes_all_products\")) ORDER BY \"offer_range\".\"name\" ASC",
        "database_type": "postgresql",
        "server_version": "14.0.12"
      },
